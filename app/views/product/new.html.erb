<h1>Add Product</h1>
<%= form_for @product do |f| %>
<p>
<%= f.label :title %>
<%= f.text_field :title%>
</p>

<p>
<%= f.label :description %>
<%= f.text_field :description %>
</p>

<p>
<%= f.label :price %>
<%= f.text_field :price%>
</p>

<p>
<%= f.label :img %>
<%= f.text_field :img%>
</p>

 <%= f.submit “Create” %>
<% end %>